msgid "\t%+#D"
msgstr "\t%+#D"

msgid "\n"
msgstr "\n"

msgid "Command: [/usr/bin/vmstat] not prepared."
msgstr "[/usr/bin/vmstat] 不存在或没有执行权限."

msgid "uplimit [${uplimit}] should be int and between 0-100."
msgstr "uplimit配置 [${uplimit}] 应该是1-100之间的整数."

msgid "wa_uplimit [${wa_uplimit}] should be int and between 0-100."
msgstr "wa_uplimit配置 [${wa_uplimit}] 应该是1-100之间的整数."

msgid "sy_uplimit [${sy_uplimit}] should be int and between 0-100."
msgstr "sy_uplimit配置 [${sy_uplimit}] 应该是1-100之间的整数."

msgid "us_uplimit [${us_uplimit}] should be int and between 0-100."
msgstr "us_uplimit配置 [${us_uplimit}] 应该是1-100之间的整数."

msgid "cpu usage: [${p_usage}%] >= uplimit: [${uplimit}%]"
msgstr "CPU整体使用率为 ${p_usage}%, 超出上限 ${uplimit}%"

msgid "cpu usage: [${p_usage}%] <= uplimit: [${uplimit}%]" 
msgstr "CPU整体使用率为 ${p_usage}%"

msgid "cpu wait usage: [${wa_usage}%] >= uplimit: [${wa_uplimit}%]"
msgstr "CPU等待占用率为 ${wa_usage}%, 超出上限 ${wa_uplimit}%"

msgid "cpu wait usage: [${wa_usage}%] <= uplimit: [${wa_uplimit}%]"
msgstr "CPU等待占用率为 ${wa_usage}%"

msgid "cpu sys usage: [${sy_usage}%] >= uplimit: [${sy_uplimit}%]"
msgstr "CPU系统态使用率为 ${sy_usage}%, 超出上限 ${sy_uplimit}%"

msgid "cpu sys usage: [${sy_usage}%] <= uplimit: [${sy_uplimit}%]"
msgstr "CPU系统态使用率为 ${sy_usage}%"

msgid "cpu user usage: [${us_usage}%] >= uplimit: [${us_uplimit}%]"
msgstr "CPU用户态使用率为 ${us_usage}%, 超出上限 ${us_uplimit}%"

msgid "cpu user usage: [${us_usage}%] <= uplimit: [${us_uplimit}%]" 
msgstr "CPU用户态使用率为 ${us_usage}%"

msgid "CPU Usage Check WARNING"
msgstr "CPU使用情况告警"

msgid "cpu usage top10:"
msgstr "CPU占用最高的10个进程:"

msgid "${warnnum}/${total} check failed."
msgstr "${warnnum}/${total} 项检查告警."

msgid "CPU Usage Check OK"
msgstr "CPU使用情况正常"

msgid "${oknum}/${total} check success."
msgstr "${oknum}/${total} 项检查正常."
