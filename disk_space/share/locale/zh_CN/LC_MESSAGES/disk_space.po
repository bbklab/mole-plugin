msgid "\t%+#D"
msgstr "\t%+#D"

msgid "\n"
msgstr "\n"

msgid "Util: [/bin/df] not prepared."
msgstr "[/bin/df] 不存在或没有执行权限."

msgid "File: [/etc/mtab] not prepared."
msgstr "[/etc/mtab] 文件不存在或为空文件."

msgid "Space Check"
msgstr "磁盘空间使用率检查"

msgid "mount point ${m}, filed number ${fdnum} <> 7"
msgstr "挂载点{m}, 字段个数为${fdnum}, 不等于7"

msgid "mount point ${m}, percent filed ${fpercent} unrecognized"
msgstr "挂载点{m}, 使用率字段格式无法识别: ${fpercent}"

msgid "mount point ${fmount} SKIP"
msgstr "跳过挂载点 ${fmount} 的检查"

msgid "${device} -> ${mount} (${fstype}), size: ${size}M,  used: ${used}M, used percent: ${used_pct}% > ${space_pct}%"
msgstr "设备: ${device}, 挂载点为${mount}, 分区类型${fstype}, 总大小${size}M, 已用${used}M, 已用百分比 ${used_pct}% > ${space_pct}%"

msgid "${device} -> ${mount} (${fstype}), size: ${size}M, used: ${used}M, used percent: ${used_pct}%"
msgstr "设备: ${device}, 挂载点为${mount}, 分区类型${fstype}, 总大小${size}M, 已用${used}M, 已用百分比 ${used_pct}%"

msgid "Inode Check"
msgstr "磁盘节点使用率检查"

msgid "${device} -> ${mount} (${fstype}), size: ${size}, used: ${used}, used percent: ${used_pct}% > ${inode_pct}%"
msgstr "设备: ${device}, 挂载点为${mount}, 分区类型${fstype}, 节点总数${size}, 已用${used}, 已用百分比 ${used_pct}% > ${inode_pct}%"

msgid "${device} -> ${mount} (${fstype}), size: ${size}, used: ${used}, used percent: ${used_pct}%"
msgstr "设备: ${device}, 挂载点为${mount}, 分区类型${fstype}, 节点总数${size}, 已用${used}, 已用百分比 ${used_pct}%"

msgid "Disk Space Check WARNING"
msgstr "磁盘使用率检查告警"

msgid "${warnnum} check failed."
msgstr "${warnnum} 项检查告警."

msgid "Disk Space Check OK"
msgstr "磁盘使用率检查正常"

msgid "disk-space, disk-inode check success."
msgstr "磁盘空间使用率,磁盘节点使用率正常."
